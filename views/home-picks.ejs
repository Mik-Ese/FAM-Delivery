<% const pick0a = ["Top Picks", "Our Customers Favourites"];
   const pick1a = ["Healthy", "Our Healthiest Selections"]; %>

<div class="section-3">
  <div class="container">
    <% for(let i = 0; i < 2; i++) { %>
    <div class="header">
      <div class="headerTitle">
        <h2>Top Picks</h2>
        <span>Our Customers Favourites</span>
      </div>
    </div>
    <div class="section-3-grid">
      <div class="section-3-col">
        <% for(let j = 0; j < 4; j++) { %>
        <div class="section-3-grid-element">
          <div class="image">
            <img src="assets/listing.jfif" alt="Listing pic"/>
          </div>
          <div class="text">
            <div class="text-title">
              <h3>Item</h3>
              <div class="info">
                <span>4.99$ | 20-30 MINS </span>
              </div>
            </div>
            <div class="rating">
              <span class="circle">4.2</span>
            </div>
          </div>
        </div>
        <% } %>
      </div>
    </div>
      <% } %>
  </div>
</div>
